def __getattr__(self, name):
    try:
        attr_function = self.dictSpecialAttrs[name]
    except KeyError:
        try:
            attr = SCons.Util.Proxy.__getattr__(self, name)
        except AttributeError as e:
            raise EntryProxyAttributeError(self, name)
        return attr
    else:
        return attr_function(self)
