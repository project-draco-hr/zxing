def str_to_node(self, s):
    top = self.fs.Top
    root = top.root
    if do_splitdrive:
        (drive, s) = os.path.splitdrive(s)
        if drive:
            root = self.fs.get_root(drive)
    if (not os.path.isabs(s)):
        s = ((top.labspath + '/') + s)
    return root._lookup_abs(s, Entry)
