{
  expTable=new int[256];
  logTable=new int[256];
  int x=1;
  for (int i=0; i < 256; i++) {
    expTable[i]=x;
    x<<=1;
    if (x >= 0x100) {
      x^=primitive;
    }
  }
  for (int i=0; i < 255; i++) {
    logTable[expTable[i]]=i;
  }
  zero=new GF256Poly(this,new int[]{0});
  one=new GF256Poly(this,new int[]{1});
}
