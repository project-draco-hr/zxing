def generate(env):
    '\n    Add Builders and construction variables for Visual Age linker to\n    an Environment.\n    '
    link.generate(env)
    env['LINKFLAGS'] = SCons.Util.CLVar('-Wl,+s -Wl,+vnocompatwarnings')
    env['SHLINKFLAGS'] = SCons.Util.CLVar('$LINKFLAGS -b')
    env['SHLIBSUFFIX'] = '.sl'
