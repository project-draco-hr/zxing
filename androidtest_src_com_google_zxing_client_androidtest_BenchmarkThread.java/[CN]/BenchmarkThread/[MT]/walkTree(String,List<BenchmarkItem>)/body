{
  File file=new File(path);
  if (file.isDirectory()) {
    String[] files=file.list();
    Arrays.sort(files);
    for (    String f : files) {
      walkTree(file.getAbsolutePath() + '/' + f,items);
    }
  }
 else {
    BenchmarkItem item=decode(path);
    if (item != null) {
      items.add(item);
    }
  }
}
