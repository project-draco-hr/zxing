{
  File file=new File(currentPath);
  if (file.isDirectory()) {
    String[] files=file.list();
    Arrays.sort(files);
    for (    String fileName : files) {
      walkTree(file.getAbsolutePath() + '/' + fileName,items);
    }
  }
 else {
    BenchmarkItem item=decode(currentPath);
    if (item != null) {
      items.add(item);
    }
  }
}
