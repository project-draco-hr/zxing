{
  if (erasures.length > numECCodewords / 2 + MAX_ERRORS || numECCodewords < 0 || numECCodewords > MAX_EC_CODEWORDS) {
    throw FormatException.getFormatInstance();
  }
  int result=0;
  int numErasures=erasures.length;
  if (result > 0) {
    numErasures-=result;
  }
  if (numErasures > MAX_ERRORS) {
    throw FormatException.getFormatInstance();
  }
  return result;
}
