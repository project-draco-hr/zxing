def postprocess(self):
    if self.top:
        t = self.targets[0]
        for tp in self.options.tree_printers:
            tp.display(t)
        if self.options.debug_includes:
            tree = t.render_include_tree()
            if tree:
                print 
                print tree
    SCons.Taskmaster.OutOfDateTask.postprocess(self)
