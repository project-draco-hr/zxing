{
  int width=image.getWidth();
  int start=x;
  int transitions=0;
  boolean black=true;
  while (start < width - 1 && transitions < 8) {
    start++;
    boolean newBlack=image.get(start,y);
    if (black != newBlack) {
      transitions++;
    }
    black=newBlack;
  }
  if (start == width - 1) {
    throw NotFoundException.getNotFoundInstance();
  }
  return start;
}
