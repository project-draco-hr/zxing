{
  if (version < 7) {
    return;
  }
  BitVector versionInfoBits=new BitVector();
  makeVersionInfoBits(version,versionInfoBits);
  int bitIndex=6 * 3 - 1;
  for (int i=0; i < 6; ++i) {
    for (int j=0; j < 3; ++j) {
      int bit=versionInfoBits.at(bitIndex);
      bitIndex--;
      matrix.set(matrix.height() - 11 + j,i,bit);
      matrix.set(i,matrix.height() - 11 + j,bit);
    }
  }
}
