{
  if (!(o instanceof BitMatrix)) {
    return false;
  }
  BitMatrix other=(BitMatrix)o;
  if (width != other.width || height != other.height || rowSize != other.rowSize || bits.length != other.bits.length) {
    return false;
  }
  for (int i=0; i < bits.length; i++) {
    if (bits[i] != other.bits[i]) {
      return false;
    }
  }
  return true;
}
