{
  boolean unlatch=false;
  do {
    if (bits.available() <= 16) {
      return;
    }
    for (int i=0; i < 4; i++) {
      int edifactValue=bits.readBits(6);
      if (edifactValue == 0x1F) {
        unlatch=true;
      }
      if (!unlatch) {
        if ((edifactValue & 0x20) == 0) {
          edifactValue|=0x40;
        }
        result.append((char)edifactValue);
      }
    }
  }
 while (!unlatch && bits.available() > 0);
}
