{
  if (textSeries != null && !textSeries.isEmpty()) {
    boolean first=true;
    StringBuilder authorsText=new StringBuilder();
    for (    String author : textSeries) {
      if (first) {
        first=false;
      }
 else {
        authorsText.append(", ");
      }
      authorsText.append(author);
    }
    texts.add(authorsText.toString());
  }
}
