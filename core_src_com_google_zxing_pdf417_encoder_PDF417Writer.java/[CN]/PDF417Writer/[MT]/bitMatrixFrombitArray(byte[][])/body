{
  int whiteSpace=30;
  BitMatrix output=new BitMatrix(input[0].length + 2 * whiteSpace,input.length + 2 * whiteSpace);
  output.clear();
  for (int y=0; y < input.length; y++) {
    for (int x=0; x < input[0].length; x++) {
      if (input[y][x] == 1) {
        output.set(x + whiteSpace,y + whiteSpace);
      }
    }
  }
  return output;
}
