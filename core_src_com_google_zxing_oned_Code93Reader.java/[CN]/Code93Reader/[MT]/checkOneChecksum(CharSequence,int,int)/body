{
  int weight=1;
  int total=0;
  for (int i=checkPosition - 1; i >= 0; i--) {
    total+=weight * ALPHABET_STRING.indexOf(result.charAt(i));
    if (++weight > weightMax) {
      weight=1;
    }
  }
  if (result.charAt(checkPosition) != ALPHABET[total % 47]) {
    throw ChecksumException.getChecksumInstance();
  }
}
