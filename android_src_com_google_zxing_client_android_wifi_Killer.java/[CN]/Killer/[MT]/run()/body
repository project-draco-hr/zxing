{
  final Handler handler=new Handler();
  Timer t=new Timer();
  t.schedule(new TimerTask(){
    @Override public void run(){
      handler.post(new Runnable(){
        public void run(){
          launchIntent(new Intent(Intent.ACTION_VIEW,Uri.parse("http://www.google.com/")));
        }
      }
);
    }
  }
,DELAY_MS);
}
