{
  Log.info("Setting resolution to min.");
  accessResolutionMenu();
  sleep(KEY_PAUSE_TIME_MS);
  keyUpAndDown(Characters.CONTROL_UP);
  sleep(KEY_PAUSE_TIME_MS);
  keyUpAndDown(Characters.CONTROL_UP);
  sleep(KEY_PAUSE_TIME_MS);
  trackBallClick();
  sleep(KEY_PAUSE_TIME_MS);
  trackBallClick();
  sleep(KEY_PAUSE_TIME_MS);
  keyUpAndDown(Characters.CONTROL_DOWN);
  sleep(KEY_PAUSE_TIME_MS);
  keyUpAndDown(Characters.CONTROL_DOWN);
  sleep(KEY_PAUSE_TIME_MS);
  trackBallClick();
  sleep(KEY_PAUSE_TIME_MS);
  keyUpAndDown(Characters.ESCAPE);
  trackBallClick();
}
