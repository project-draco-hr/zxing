def outdent(self, s):
    '\n        Strip as many spaces from each line as are found at the beginning\n        of the first line in the list.\n        '
    lines = s.split('\n')
    if (lines[0] == ''):
        lines = lines[1:]
    spaces = re.match(' *', lines[0]).group(0)

    def strip_initial_spaces(l, s=spaces):
        if l.startswith(spaces):
            l = l[len(spaces):]
        return l
    return ('\n'.join([strip_initial_spaces(l) for l in lines]) + '\n')
