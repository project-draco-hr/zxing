{
  Formatter formatter=new Formatter();
  int row=0;
  for (  Codeword codeword : codewords) {
    if (codeword == null) {
      formatter.format("%3d:    |   \n",row++);
      continue;
    }
    formatter.format("%3d: %3d|%3d\n",row++,codeword.getRowNumber(),codeword.getValue());
  }
  String result=formatter.toString();
  formatter.close();
  return result;
}
