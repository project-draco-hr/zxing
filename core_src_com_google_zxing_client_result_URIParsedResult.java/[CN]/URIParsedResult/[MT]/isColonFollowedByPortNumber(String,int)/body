{
  int nextSlash=uri.indexOf('/',protocolEnd + 1);
  if (nextSlash < 0) {
    nextSlash=uri.length();
  }
  if (nextSlash <= protocolEnd + 1) {
    return false;
  }
  for (int x=protocolEnd + 1; x < nextSlash; x++) {
    if (uri.charAt(x) < '0' || uri.charAt(x) > '9') {
      return false;
    }
  }
  return true;
}
