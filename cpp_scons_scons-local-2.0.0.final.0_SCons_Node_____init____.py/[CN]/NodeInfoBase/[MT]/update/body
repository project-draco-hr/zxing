def update(self, node):
    try:
        field_list = self.field_list
    except AttributeError:
        return
    for f in field_list:
        try:
            delattr(self, f)
        except AttributeError:
            pass
        try:
            func = getattr(node, ('get_' + f))
        except AttributeError:
            pass
        else:
            setattr(self, f, func())
