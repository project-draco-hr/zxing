def __init__(self):
    if __debug__:
        logInstanceCreation(self, 'Node.Node')
    self.sources = []
    self.sources_set = set()
    self._specific_sources = False
    self.depends = []
    self.depends_set = set()
    self.ignore = []
    self.ignore_set = set()
    self.prerequisites = SCons.Util.UniqueList()
    self.implicit = None
    self.waiting_parents = set()
    self.waiting_s_e = set()
    self.ref_count = 0
    self.wkids = None
    self.env = None
    self.state = no_state
    self.precious = None
    self.noclean = 0
    self.nocache = 0
    self.always_build = None
    self.includes = None
    self.attributes = self.Attrs()
    self.side_effect = 0
    self.side_effects = []
    self.linked = 0
    self.clear_memoized_values()
    Annotate(self)
