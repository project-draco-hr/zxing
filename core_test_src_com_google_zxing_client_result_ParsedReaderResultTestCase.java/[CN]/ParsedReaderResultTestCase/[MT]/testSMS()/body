{
  doTestResult("sms:+15551212",ParsedResultType.SMS);
  doTestResult("SMS:+15551212",ParsedResultType.SMS);
  doTestResult("SMSTO:+15551212",ParsedResultType.SMS);
  doTestResult("smsto:+15551212",ParsedResultType.SMS);
