{
  int currentPos=initialPos;
  boolean current=row.get(currentPos);
  while (currentPos < row.getSize() && row.get(currentPos) == current) {
    currentPos++;
  }
  current=!current;
  while (currentPos < row.getSize() && row.get(currentPos) == current) {
    currentPos++;
  }
  return currentPos;
}
