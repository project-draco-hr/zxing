{
  while (true) {
    ExpandedPair nextPair=retrieveNextPair(row,this.pairs,rowNumber);
    this.pairs.add(nextPair);
    if (nextPair.mayBeLast()) {
      if (checkChecksum()) {
        return this.pairs;
      }
      if (nextPair.mustBeLast()) {
        throw NotFoundException.getNotFoundInstance();
      }
    }
  }
}
