{
  latitude.addStyleName(StylesDefs.INPUT_FIELD_REQUIRED);
  latitude.addChangeHandler(handler);
  latitude.addKeyPressHandler(keyListener);
  longitude.addStyleName(StylesDefs.INPUT_FIELD_REQUIRED);
