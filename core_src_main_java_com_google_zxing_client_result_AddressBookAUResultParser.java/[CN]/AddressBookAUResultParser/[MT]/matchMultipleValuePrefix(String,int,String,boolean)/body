{
  List<String> values=null;
  for (int i=1; i <= max; i++) {
    String value=matchSinglePrefixedField(prefix + i + ':',rawText,'\r',trim);
    if (value == null) {
      break;
    }
    if (values == null) {
      values=new ArrayList<String>(max);
    }
    values.add(value);
  }
  if (values == null) {
    return null;
  }
  return values.toArray(new String[values.size()]);
}
