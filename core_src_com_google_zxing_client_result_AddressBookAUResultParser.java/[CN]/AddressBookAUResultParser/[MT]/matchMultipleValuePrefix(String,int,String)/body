{
  Vector values=null;
  for (int i=1; i <= max; i++) {
    String value=matchSinglePrefixedField(prefix + i + ':',rawText,'\r');
    if (value == null) {
      break;
    }
    if (values == null) {
      values=new Vector(max);
    }
    values.addElement(value);
  }
  if (values == null) {
    return null;
  }
  return toStringArray(values);
}
