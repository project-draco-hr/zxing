{
switch (bits) {
case 0x0:
    return TERMINATOR;
case 0x1:
  return NUMERIC;
case 0x2:
return ALPHANUMERIC;
case 0x4:
return BYTE;
case 0x5:
return FNC1_FIRST_POSITION;
case 0x7:
return ECI;
case 0x8:
return KANJI;
case 0x9:
return FNC1_SECOND_POSITION;
default :
throw new IllegalArgumentException();
}
}
