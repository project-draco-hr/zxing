def output_to_dict(output):
    'Given an output string, parse it to find env variables.\n\n    Return a dict where keys are variables names, and values their content'
    envlinem = re.compile('^([a-zA-z0-9]+)=([\\S\\s]*)$')
    parsedenv = {}
    for line in output.splitlines():
        m = envlinem.match(line)
        if m:
            parsedenv[m.group(1)] = m.group(2)
    return parsedenv
