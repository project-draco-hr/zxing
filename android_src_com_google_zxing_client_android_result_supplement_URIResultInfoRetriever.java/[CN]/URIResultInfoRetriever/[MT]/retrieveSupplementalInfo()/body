{
  String oldURI=result.getURI();
  String newURI=unredirect(oldURI);
  int count=0;
  while (count < 3 && !oldURI.equals(newURI)) {
    append(result.getDisplayResult(),null,new String[]{redirectString + " : " + newURI},newURI);
    count++;
    oldURI=newURI;
    newURI=unredirect(newURI);
  }
}
