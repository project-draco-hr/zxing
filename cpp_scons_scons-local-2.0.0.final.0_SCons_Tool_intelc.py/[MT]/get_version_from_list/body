def get_version_from_list(v, vlist):
    'See if we can match v (string) in vlist (list of strings)\n    Linux has to match in a fuzzy way.'
    if is_windows:
        if (v in vlist):
            return v
        else:
            return None
    else:
        fuzz = 0.001
        for vi in vlist:
            if (math.fabs((linux_ver_normalize(vi) - linux_ver_normalize(v))) < fuzz):
                return vi
        return None
