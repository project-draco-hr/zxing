def Build(self):
    try:
        self.file = open(self.dspabs, 'w')
    except IOError as detail:
        raise SCons.Errors.InternalError(((('Unable to open "' + self.dspabs) + '" for writing:') + str(detail)))
    else:
        self.PrintHeader()
        self.PrintProject()
        self.file.close()
