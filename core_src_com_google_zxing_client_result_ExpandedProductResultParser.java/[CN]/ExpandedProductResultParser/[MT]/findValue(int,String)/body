{
  StringBuffer buf=new StringBuffer();
  String rawTextAux=rawText.substring(i);
  for (int index=0; index < rawTextAux.length(); index++) {
    char c=rawTextAux.charAt(index);
    if (c == '(') {
      if (findAIvalue(index,rawTextAux) == null) {
        buf.append('(');
      }
 else {
        break;
      }
    }
 else {
      buf.append(c);
    }
  }
  return buf.toString();
}
