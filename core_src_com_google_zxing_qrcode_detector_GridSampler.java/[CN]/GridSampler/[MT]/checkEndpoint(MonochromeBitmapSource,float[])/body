{
  int x=(int)points[0];
  int y=(int)points[1];
  if (x < 0 || x >= image.getWidth() || y < 0 || y >= image.getHeight()) {
    throw new ReaderException("Transformed point out of bounds at " + x + ','+ y);
  }
  x=(int)points[points.length - 2];
  y=(int)points[points.length - 1];
  if (x < 0 || x >= image.getWidth() || y < 0 || y >= image.getHeight()) {
    throw new ReaderException("Transformed point out of bounds at " + x + ','+ y);
  }
}
