{
{
    BitVector bits=new BitVector();
    Encoder.appendLengthInfo(1,1,QRCode.MODE_NUMERIC,bits);
    assertEquals("0000000001",bits.toString());
  }
{
    BitVector bits=new BitVector();
    Encoder.appendLengthInfo(2,10,QRCode.MODE_ALPHANUMERIC,bits);
    assertEquals("00000000010",bits.toString());
  }
{
    BitVector bits=new BitVector();
    Encoder.appendLengthInfo(255,27,QRCode.MODE_8BIT_BYTE,bits);
    assertEquals("0000000011111111",bits.toString());
  }
{
    BitVector bits=new BitVector();
    Encoder.appendLengthInfo(1024,40,QRCode.MODE_KANJI,bits);
    assertEquals("001000000000",bits.toString());
  }
}
