{
  if (size != poly.degree() + 1) {
    return false;
  }
  for (int i=0; i < size; ++i) {
    final int coeff=GaloisField.GetField(8).Log(poly.coeff(size - i - 1));
    if (expected[i] != coeff) {
      Debug.LOG_ERROR("values don't match at " + i + ": "+ expected[i]+ " vs. "+ coeff);
      return false;
    }
  }
  return true;
}
