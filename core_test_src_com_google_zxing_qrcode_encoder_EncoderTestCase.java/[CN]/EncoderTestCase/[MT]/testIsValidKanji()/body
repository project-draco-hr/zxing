{
  assertTrue(Encoder.isValidKanji(0x82,0xa0));
  assertTrue(Encoder.isValidKanji(0x93,0xfa));
  assertTrue(Encoder.isValidKanji(0x8a,0xbf));
  assertTrue(Encoder.isValidKanji(0xe7,0x4e));
  assertTrue(Encoder.isValidKanji(0xea,0xa2));
  assertFalse(Encoder.isValidKanji('0','1'));
  assertFalse(Encoder.isValidKanji(0x82,0x7f));
  assertFalse(Encoder.isValidKanji(0xa0,0xa0));
}
