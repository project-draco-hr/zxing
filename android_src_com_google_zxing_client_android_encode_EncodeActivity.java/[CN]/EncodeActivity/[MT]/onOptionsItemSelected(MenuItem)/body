{
switch (item.getItemId()) {
case R.id.menu_share:
    share();
  return true;
case R.id.menu_encode:
Intent intent=getIntent();
if (intent == null) {
return false;
}
intent.putExtra(USE_VCARD_KEY,!qrCodeEncoder.isUseVCard());
startActivity(intent);
finish();
return true;
default :
return false;
}
}
