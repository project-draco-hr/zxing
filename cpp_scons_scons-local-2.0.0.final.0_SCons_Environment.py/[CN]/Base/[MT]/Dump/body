def Dump(self, key=None):
    '\n        Using the standard Python pretty printer, dump the contents of the\n        scons build environment to stdout.\n\n        If the key passed in is anything other than None, then that will\n        be used as an index into the build environment dictionary and\n        whatever is found there will be fed into the pretty printer. Note\n        that this key is case sensitive.\n        '
    import pprint
    pp = pprint.PrettyPrinter(indent=2)
    if key:
        dict = self.Dictionary(key)
    else:
        dict = self.Dictionary()
    return pp.pformat(dict)
