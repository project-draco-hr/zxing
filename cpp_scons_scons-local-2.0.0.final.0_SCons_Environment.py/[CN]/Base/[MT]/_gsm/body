def _gsm(self):
    try:
        return self._memo['_gsm']
    except KeyError:
        pass
    result = {}
    try:
        scanners = self._dict['SCANNERS']
    except KeyError:
        pass
    else:
        if (not SCons.Util.is_List(scanners)):
            scanners = [scanners]
        else:
            scanners = scanners[:]
        scanners.reverse()
        for scanner in scanners:
            for k in scanner.get_skeys(self):
                if (k and (self['PLATFORM'] == 'win32')):
                    k = k.lower()
                result[k] = scanner
    self._memo['_gsm'] = result
    return result
