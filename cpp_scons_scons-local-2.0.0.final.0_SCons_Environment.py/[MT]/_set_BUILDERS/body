def _set_BUILDERS(env, key, value):
    try:
        bd = env._dict[key]
        for k in bd.keys():
            del bd[k]
    except KeyError:
        bd = BuilderDict(kwbd, env)
        env._dict[key] = bd
    for (k, v) in value.items():
        if (not SCons.Builder.is_a_Builder(v)):
            raise SCons.Errors.UserError(('%s is not a Builder.' % repr(v)))
    bd.update(value)
