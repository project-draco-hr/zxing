{
  float yi=y.getX();
  float yj=y.getY();
  float zi=z.getX();
  float zj=z.getY();
  float xi=x.getX();
  float xj=x.getY();
  float ti=t.getX();
  float tj=t.getY();
  if (yi < width / 2.0f) {
    return new ResultPoint[]{new ResultPoint(ti - CORR,tj + CORR),new ResultPoint(zi + CORR,zj + CORR),new ResultPoint(xi - CORR,xj - CORR),new ResultPoint(yi + CORR,yj - CORR)};
  }
 else {
    return new ResultPoint[]{new ResultPoint(ti + CORR,tj + CORR),new ResultPoint(zi + CORR,zj - CORR),new ResultPoint(xi - CORR,xj + CORR),new ResultPoint(yi - CORR,yj - CORR)};
  }
}
