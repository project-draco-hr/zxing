{
  float yi=y.getX();
  float yj=y.getY();
  float zi=z.getX();
  float zj=z.getY();
  float xi=x.getX();
  float xj=x.getY();
  float ti=t.getX();
  float tj=t.getY();
  final int corr=1;
  if (yi < width / 2) {
    return new ResultPoint[]{new ResultPoint(ti - corr,tj + corr),new ResultPoint(zi + corr,zj + corr),new ResultPoint(xi - corr,xj - corr),new ResultPoint(yi + corr,yj - corr)};
  }
 else {
    return new ResultPoint[]{new ResultPoint(ti + corr,tj + corr),new ResultPoint(zi + corr,zj - corr),new ResultPoint(xi - corr,xj + corr),new ResultPoint(yi - corr,yj - corr)};
  }
}
