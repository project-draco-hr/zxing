{
  if (horizontal) {
    for (int x=b + 1; x >= a; x--) {
      if (image.get(x,fixed)) {
        return new ResultPoint(x,fixed);
      }
    }
  }
 else {
    for (int y=b + 1; y >= a; y--) {
      if (image.get(fixed,y)) {
        return new ResultPoint(fixed,y);
      }
    }
  }
  return null;
}
