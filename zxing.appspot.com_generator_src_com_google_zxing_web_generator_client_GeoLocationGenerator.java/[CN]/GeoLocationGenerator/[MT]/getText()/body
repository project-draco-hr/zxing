{
  String que=getQueryField();
  if (null != que && que.length() > 0) {
    if (null == getLatitudeField()) {
      latitude.setText("0");
    }
    if (null == getLongitudeField()) {
      longitude.setText("0");
    }
  }
  String lat=getLatitudeField();
  String lon=getLongitudeField();
  if (null != que && que.length() > 0) {
    return "geo:" + lat + ','+ lon+ "?q="+ que;
  }
  return "geo:" + lat + ','+ lon;
}
