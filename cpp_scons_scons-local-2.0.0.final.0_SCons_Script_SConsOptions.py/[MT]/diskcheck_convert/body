def diskcheck_convert(value):
    if (value is None):
        return []
    if (not SCons.Util.is_List(value)):
        value = value.split(',')
    result = []
    for v in value:
        v = v.lower()
        if (v == 'all'):
            result = diskcheck_all
        elif (v == 'none'):
            result = []
        elif (v in diskcheck_all):
            result.append(v)
        else:
            raise ValueError(v)
    return result
