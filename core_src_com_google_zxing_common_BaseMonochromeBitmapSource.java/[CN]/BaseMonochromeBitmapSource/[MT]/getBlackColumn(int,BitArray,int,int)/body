{
  if (column == null || column.getSize() < getHeight) {
    column=new BitArray(getHeight);
  }
 else {
    column.clear();
  }
  initLuminances();
  int[] localLuminances=getLuminanceColumn(x,luminances);
  for (int y=0; y < getHeight; y++) {
    if (localLuminances[startY + y] < blackPoint) {
      column.set(y);
    }
  }
  return column;
}
