def createIncludesFromHeaders(headers, leaveLast, include_quotes='""'):
    if (not SCons.Util.is_List(headers)):
        headers = [headers]
    l = []
    if leaveLast:
        lastHeader = headers[(-1)]
        headers = headers[:(-1)]
    else:
        lastHeader = None
    for s in headers:
        l.append(('#include %s%s%s\n' % (include_quotes[0], s, include_quotes[1])))
    return (''.join(l), lastHeader)
