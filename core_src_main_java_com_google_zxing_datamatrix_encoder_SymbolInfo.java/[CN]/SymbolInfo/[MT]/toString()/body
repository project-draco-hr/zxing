{
  return (rectangular ? "Rectangular Symbol:" : "Square Symbol:") + " data region " + matrixWidth+ 'x'+ matrixHeight+ ", symbol size "+ getSymbolWidth()+ 'x'+ getSymbolHeight()+ ", symbol data size "+ getSymbolDataWidth()+ 'x'+ getSymbolDataHeight()+ ", codewords "+ dataCapacity+ '+'+ errorCodewords;
}
