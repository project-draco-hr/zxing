def generate(env):
    SCons.Tool.bcc32.findIt('tlib', env)
    'Add Builders and construction variables for ar to an Environment.'
    SCons.Tool.createStaticLibBuilder(env)
    env['AR'] = 'tlib'
    env['ARFLAGS'] = SCons.Util.CLVar('')
    env['ARCOM'] = '$AR $TARGET $ARFLAGS /a $SOURCES'
    env['LIBPREFIX'] = ''
    env['LIBSUFFIX'] = '.lib'
