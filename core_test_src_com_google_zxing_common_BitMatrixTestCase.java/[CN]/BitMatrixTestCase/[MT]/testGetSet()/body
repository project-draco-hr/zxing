{
  BitMatrix matrix=new BitMatrix(33);
  assertEquals(33,matrix.getDimension());
  for (int i=0; i < 33; i++) {
    for (int j=0; j < 33; j++) {
      if (i * j % 3 == 0) {
        matrix.set(j,i);
      }
    }
  }
  for (int i=0; i < 33; i++) {
    for (int j=0; j < 33; j++) {
      assertEquals(i * j % 3 == 0,matrix.get(j,i));
    }
  }
}
