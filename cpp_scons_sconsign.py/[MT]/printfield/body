def printfield(name, entry, prefix=''):
    outlist = field('implicit', entry, 0)
    if outlist:
        if Verbose:
            print '    implicit:'
        print ('        ' + outlist)
    outact = field('action', entry, 0)
    if outact:
        if Verbose:
            print ('    action: ' + outact)
        else:
            print ('        ' + outact)
